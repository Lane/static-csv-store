{
  "name": "static-csv-store",
  "version": "1.0.2",
  "description": "A static file data store for react components that handles data loading and selection",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "deploy": "nwb build-react-component && surge demo/dist --domain static-csv-store-demo.surge.sh",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "d3-dsv": "^1.2.0",
    "zustand": "^2.2.3"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "0.25.x",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "author": "Lane Olson",
  "homepage": "https://github.com/Lane/static-csv-store",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "zustand",
    "data store",
    "react"
  ]
}
